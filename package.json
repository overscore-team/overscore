{
  "name": "@overscore-team/overscore",
  "version": "0.0.7",
  "description": "",
  "private": false,
  "main": "dist/overscore.min.js",
  "scripts": {
    "cz": "git cz",
    "test": "jest",
    "build": "webpack --config ./config/webpack.config.js",
    "lint-staged": "lint-staged",
    "commintlint": "commitlint --config commitlint.config.js -e -V",
    "prepare": "husky install",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/overscore-team/overscore.git"
  },
  "keywords": [
    "overscore",
    "tools"
  ],
  "author": "overscore-team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/overscore-team/overscore/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/overscore-team/overscore#readme",
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.11",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "babel-loader": "^8.2.3",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.2.2",
    "core-js": "^2.6.12",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.6",
    "prettier": "^2.6.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e -V && husky commit-msg -e -V",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "node -v",
      "prettier --write",
      "eslint --cache --fix --config .eslintrc.js"
    ]
  }
}
